            <Font name="Fira Code Retina" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></PasswordField>
      <Button layoutX="248.0" layoutY="282.0" mnemonicParsing="false" onAction="#handleRegisterButton" prefHeight="34.0" prefWidth="111.0" style="-fx-background-color: #ffcccc; -fx-border-color: #663366; -fx-border-radius: 15; -fx-background-radius: 15;" text="SUBMIT" textFill="#663366">
