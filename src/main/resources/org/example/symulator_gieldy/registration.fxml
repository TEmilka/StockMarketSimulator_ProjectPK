<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffe6f2;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.symulator_gieldy.RegistrationController">
   <children>
      <PasswordField fx:id="passwordField" layoutX="164.0" layoutY="174.0" prefHeight="25.0" prefWidth="278.0" promptText="Password" style="-fx-background-color: rgba(255,204,204,0.5); -fx-border-color: #996699;">
         <font>
            <Font name="Fira Code Regular" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></PasswordField>
      <PasswordField fx:id="checkPasswordField" layoutX="163.0" layoutY="220.0" prefHeight="34.0" prefWidth="278.0" promptText="Confirm password" style="-fx-background-color: rgba(255,204,204,0.5); -fx-border-color: #996699;">
